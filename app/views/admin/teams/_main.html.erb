<div>
  <table class="table table-hover table-admin">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Country</th>
        <th>Created_at</th>
      </tr>
    </thead>
    <% @teams.each do |team| %>
      <tr>
        <td>
          <%= team.id %>
        </td>
        <td>
          <%= team.name %>
        </td>
        <td>
          <%= team.country %>
        </td>
        <td>
          <%= team.created_at %>
        </td>
        <td>
          <%= link_to "View", team_path(team) %>
          <%= link_to "Edit", edit_admin_team_path(team) %>
          <%= link_to "Delete", admin_team_path(team), method: :delete, data: {confirm: "You sure?"} %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
